<div class="container" style="padding: 10% 0% 10% 0%">
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th scope="col">ID Number</th>
            <th scope="col">Length</th>
            <th scope="col">Material</th>
            <th scope="col">Core</th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
        </thead>
        <tbody>
        {{#each wand}}
            <tr>
                <td>{{wID}}</td>
                <td>{{length}}</td>
                <td>{{material}}</td>
                <td>{{core}}</td>
                <td><button type="button" class="btn btn-danger" onclick="deleteWand({{wID}})">Delete</button></td>
                <td><a class="btn btn-primary" href="/viewWand/{{wID}}" role="button">Update</a></td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
