<br />
<h2>Dismiss Professor:</h2>
<form id="dismissProfessor" action="/dismissProfessor" method="post">
    <div>
        <label class="inputLabel">Professor Name (Required):</label>
        <select name="name_remove" id="name_remove">
            <option value="" selected="selected">Select Student</option>
            {{#if countries}}
            {{#each countries}}
            <option id="selectedName" value="{{this.id}}">{{this.first_name}} {{this.last_name}}</option>
            {{/each}}
            {{/if}}
        </select>
    </div>
    <br />
    <div>
        <input type="submit" name="Dismiss Professor" value="Dismiss Professor" id="postDismissProfessor">
    </div>
</form>